[distutils]
index-servers=
    {{ cookiecutter.pypi_alias }}

[{{ cookiecutter.pypi_alias }}]
repository: {{ cookiecutter.pypi_schema }}://{{cookiecutter.pypi_host_and_path}}{{'' if cookiecutter.pypi_port in ['', '-'] else ':'+cookiecutter.pypi_port}}
username: {{ cookiecutter.pypi_login }}
password: {{ cookiecutter.pypi_password }}
